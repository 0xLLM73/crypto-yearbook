{
  "name": "crypto-yearbook",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:analyze": "vite build --mode analyze",
    "build:production": "NODE_ENV=production vite build",
    "serve": "vite preview --host 0.0.0.0 --port 3000",
    "docker:build": "docker build -t crypto-yearbook .",
    "docker:run": "docker run -p 3000:80 crypto-yearbook",
    "docker:dev": "docker-compose up --build",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod --dir=dist",
    "clean": "rm -rf dist node_modules/.vite",
    "install:legacy": "npm install --legacy-peer-deps",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "test:build": "npm run build && npm run preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --coverage --ui",
    "test:silent": "vitest run --silent",
    "test:accessibility": "node scripts/accessibility-test.js",
    "test:performance": "node scripts/performance-test.js",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:all": "npm run test:run && npm run test:accessibility && npm run test:performance",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "bundle:analyze": "npx vite-bundle-analyzer dist",
    "performance:lighthouse": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "validate": "npm run lint && npm run type-check && npm run test:run && npm run build"
  },
